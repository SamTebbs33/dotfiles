{
   "languageserver": {
       "zls" : {
           "command": "zls",
           "filetypes": ["zig"]
       },
       "clangd": {
          "command": "clangd",
          "rootPatterns": ["compile_flags.txt", "compile_commands.json"],
          "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"]
       }
  },
  "coc.source.OmniSharp.enable": true,
  "coc.source.OmniSharp.triggerCharacters": "."
}
